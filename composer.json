  "require": {
    "php": ">=7.1",
    "psr/cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
  },
  "authors": [
    {
      "name": "Damiano Petrungaro",
      "email": "damiano.petrungaro@qwentes.it"
    }
  ],
  "autoload": {
    "psr-4": {
